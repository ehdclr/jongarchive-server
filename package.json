{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "NODE_ENV=local nest start --watch",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --testPathIgnorePatterns=integration",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest __test__/integration",
    "test:all": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:db:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:db:down": "docker-compose -f docker-compose.test.yml down -v",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "if [ \"$NODE_ENV\" = \"production\" ]; then NODE_ENV=production node dist/database/migrate.js; elif [ \"$NODE_ENV\" = \"development\" ]; then NODE_ENV=development ts-node src/database/migrate.ts; else NODE_ENV=local ts-node src/database/migrate.ts; fi",
    "db:push": "if [ \"$NODE_ENV\" = \"production\" ]; then NODE_ENV=production drizzle-kit push; elif [ \"$NODE_ENV\" = \"development\" ]; then NODE_ENV=development drizzle-kit push; else NODE_ENV=local drizzle-kit push; fi",
    "db:drop": "if [ \"$NODE_ENV\" = \"production\" ]; then NODE_ENV=production drizzle-kit drop; elif [ \"$NODE_ENV\" = \"development\" ]; then NODE_ENV=development drizzle-kit drop; else NODE_ENV=local drizzle-kit drop; fi",
    "db:studio": "if [ \"$NODE_ENV\" = \"production\" ]; then NODE_ENV=production drizzle-kit studio; elif [ \"$NODE_ENV\" = \"development\" ]; then NODE_ENV=development drizzle-kit studio; else NODE_ENV=local drizzle-kit studio; fi"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.931.0",
    "@aws-sdk/s3-request-presigner": "^3.931.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "drizzle-orm": "^0.44.7",
    "got": "^14.6.3",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "redis": "^5.9.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^10.1.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^11.0.0",
    "aws-sdk-client-mock": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@faker-js/faker|uuid)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!src/**/*.module.ts",
      "!src/main.ts",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@domains/(.*)$": "<rootDir>/domains/$1",
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@config/(.*)$": "<rootDir>/config/$1",
      "^@database/(.*)$": "<rootDir>/database/$1",
      "^@test/(.*)$": "<rootDir>/__test__/$1"
    }
  }
}
